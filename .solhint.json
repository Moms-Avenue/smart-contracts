{
    "extends": "default",
    "rules": {
      "avoid-throw": false,
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "indent": ["warn", 4],
      "const-name-snakecase": false,
      "no-simple-event-func-name": false,
      "compiler-fixed": false,
      "not-rely-on-time": false,
      "not-rely-on-block-hash": false,
      "state-visibility": false,
      "func-order": false
    }
  }